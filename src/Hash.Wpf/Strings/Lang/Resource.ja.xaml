<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xml:space="preserve">

    <!-- MainWindow -->
    <sys:String x:Key="Lang.Menu.File">ファイル(_F)</sys:String>
    <sys:String x:Key="Lang.Menu.File.Settings">設定(_S)</sys:String>
    <sys:String x:Key="Lang.Menu.File.Exit">終了(_X)</sys:String>
    <sys:String x:Key="Lang.Menu.Help">ヘルプ(_H)</sys:String>
    <sys:String x:Key="Lang.Menu.Help.Readme">Readme(_R)</sys:String>
    <sys:String x:Key="Lang.Menu.Help.About">バージョン情報(_V)</sys:String>

    <sys:String x:Key="Lang.Tab.HashCalculator">Hash計算機</sys:String>
    <sys:String x:Key="Lang.Tab.HashCompare">Hash比較</sys:String>
    <sys:String x:Key="Lang.Tab.Readme">Readme</sys:String>
    <sys:String x:Key="Lang.Tab.Settings">設定</sys:String>

    <sys:String x:Key="Lang.Calculator.FilePath">ファイルのパス</sys:String>
    <sys:String x:Key="Lang.Calculator.DropFileHere">ここにファイルをドロップ</sys:String>
    <sys:String x:Key="Lang.Calculator.SelectFile">ファイルを選択</sys:String>
    <sys:String x:Key="Lang.Calculator.SelectHash">Hashを選択</sys:String>
    <sys:String x:Key="Lang.Calculator.UpperCase">大文字</sys:String>
    <sys:String x:Key="Lang.Calculator.Hyphen">ハイフン</sys:String>
    <sys:String x:Key="Lang.Calculator.Reset">値をリセット</sys:String>
    <sys:String x:Key="Lang.Calculator.OutputHash">ここにHash値が表示されます</sys:String>
    <sys:String x:Key="Lang.Calculator.Copy">Hash値をコピー</sys:String>
    <sys:String x:Key="Lang.Calculator.CopyToCompare1">比較①にコピー</sys:String>
    <sys:String x:Key="Lang.Calculator.CopyToCompare2">比較②にコピー</sys:String>

    <sys:String x:Key="Lang.Compare.Reset">リセット</sys:String>
    <sys:String x:Key="Lang.Compare.One">比較①</sys:String>
    <sys:String x:Key="Lang.Compare.One.Paste">クリップボードから①へ貼り付け</sys:String>
    <sys:String x:Key="Lang.Compare.Two">比較②</sys:String>
    <sys:String x:Key="Lang.Compare.Two.Paste">クリップボードから②へ貼り付け</sys:String>
    <sys:String x:Key="Lang.Compare.ResultBefore">比較 :</sys:String>
    <sys:String x:Key="Lang.Compare.Result">結果</sys:String>
    <sys:String x:Key="Lang.Compare.Result.True">真</sys:String>
    <sys:String x:Key="Lang.Compare.Result.False">偽</sys:String>
    <sys:String x:Key="Lang.Compare.Execute">比較</sys:String>

    <sys:String x:Key="Lang.Settings.HashForContext">Hash for Contextを有効にする</sys:String>
    <sys:String x:Key="Lang.Settings.HashForContext.MultiInstance"> (最初のインスタンスでのみ変更できます)</sys:String>

    <sys:String x:Key="Lang.HashForContext.FilePath">ファイルのパス (※これが表示されている場合は起動方法が間違っています)</sys:String>
    <sys:String x:Key="Lang.HashForContext.StartHash">HashCalculatorを起動</sys:String>

    <sys:String x:Key="Lang.About.Title">HashCalculatorについて</sys:String>
    <sys:String x:Key="Lang.About.License">ライセンス</sys:String>
